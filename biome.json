{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"formatter": {
		"indentWidth": 4
	},
	"javascript": {
		"formatter": {
			"indentWidth": 4,
			"quoteStyle": "double",
			"jsxQuoteStyle": "double",
			"semicolons": "always"
		}
	}
}